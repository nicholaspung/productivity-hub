{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.20.0",
    "firebase": "^7.20.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "firebase-deploy": "npm run build:css && craco build && firebase deploy --only hosting",
    "generate-tests": "node scripts/autoAddFunctionTests.js && node scripts/autoAddReduxReducerTests.js",
    "generate-tests:test": "node scripts/autoAddFunctionTests.js -T && node scripts/autoAddReduxReducerTests.js -T",
    "test:all": "npm run generate-tests && npm run test",
    "lint": "node_modules/.bin/eslint --ext .js,.jsx src",
    "prettier:check": "node_modules/.bin/prettier --check \"src/**/*.js*\"",
    "prettier:fix": "node_modules/.bin/prettier -w \"src/**/*.js*\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/parser": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "autoprefixer": "^9.8.6",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^7.0.35",
    "postcss-cli": "^8.0.0",
    "prettier": "2.1.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3"
  }
}
